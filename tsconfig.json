{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "assets/jss/nextjs-material-kit-pro/tooltipsStyle.js",
    "lib/apollo.jsx",
    "components/Header/LoggedOut.jsx",
    "components/Header/LoggedIn.jsx",
    "pages/index.jsx",
    "components/Unauthenticated/index.jsx",
    "components/Authenticated/index.jsx"
  ],
  "exclude": ["node_modules"]
}
